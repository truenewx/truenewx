{
  "name": "@truenewx/tnxweb",
  "version": "2.6.1",
  "description": "互联网技术解决方案：Web扩展支持",
  "private": false,
  "publishConfig": {
    "cache": "~/.npm",
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "main": "sample/main.js",
  "keywords": [
    "truenewx",
    "tnxweb",
    "tnx"
  ],
  "author": "truenewx",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "core-js": "3.22.8",
    "axios": "0.27.2",
    "base-64": "1.0.0",
    "md5": "2.3.0",
    "jquery": "3.6.0",
    "bootstrap": "5.1.3",
    "vue": "3.2.36",
    "vue-router": "4.0.15",
    "mitt": "3.0.0",
    "element-plus": "2.2.2",
    "@element-plus/icons-vue": "2.0.1",
    "ant-design-vue": "3.2.5",
    "async-validator": "4.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.15",
    "@vue/cli-plugin-eslint": "4.5.15",
    "@vue/cli-plugin-router": "4.5.15",
    "@vue/cli-service": "4.5.15",
    "@vue/compiler-sfc": "3.2.30",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "7.17.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "copy-webpack-plugin": "5.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "eqeqeq": "warn",
      "no-unused-vars": [
        "error",
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "no-undef": "warn",
      "no-useless-escape": "warn",
      "vue/no-v-model-argument": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
