{
  "name": "@truenewx/tnxweb",
  "version": "2.3.1",
  "description": "互联网技术解决方案：Web扩展支持",
  "private": false,
  "publishConfig": {
    "cache": "~/.npm",
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "main": "sample/main.js",
  "keywords": [
    "truenewx",
    "tnxweb",
    "tnx"
  ],
  "author": "truenewx",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "core-js": "3.18.1",
    "axios": "0.21.4",
    "base-64": "1.0.0",
    "md5": "2.3.0",
    "jquery": "3.6.0",
    "bootstrap": "5.1.1",
    "vue": "3.2.19",
    "vue-router": "4.0.11",
    "mitt": "3.0.0",
    "element-plus": "1.1.0-beta.18",
    "@element-plus/icons": "0.0.11",
    "ant-design-vue": "2.2.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-router": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/compiler-sfc": "3.2.11",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "7.17.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "eqeqeq": "warn",
      "no-unused-vars": [
        "error",
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "no-undef": "warn",
      "no-useless-escape": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
